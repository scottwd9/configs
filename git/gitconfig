[user]
	name = William Scott
	email = scottwd9
[alias]
	f = fetch --prune
	pu = push -u origin HEAD
	p = pull --rebase
	r = rebase @{u}
	ri = rebase -i @{u}
	s = status -sb
	ci = commit
	cim = commit -m
	cl = clean -df
	br = branch
	co = checkout
	d = diff
	dc = diff --cached --ignore-space-at-eol -b -w --ignore-blank-lines
	ds = diff --stat --ignore-space-at-eol -b -w --ignore-blank-lines
	dt = difftool
	sl = stash list
	ss = stash save
	sa = stash apply
	sp = stash pop
	sd = stash drop
	ls = ls-files
	lg = log --color --graph --pretty=format:'%C(magenta)%h%Creset %s%C(yellow)%d%Creset (%C(blue)%an%Creset %Cgreen%cr%Creset)'
	latest = for-each-ref --sort=-committerdate --format='%(committerdate:short) %(refname:short)'
	mv = branch -m
	st = status
[core]
	autocrlf = input
	editor = emacs -nw -q
	excludesfile = ~/.gitignore_global
[branch]
	autosetuprebase = always
[push]
	default = simple
[rebase]
	autostash = true
[color]
	ui = auto
