[user]
	name = William Scott
	email = scottwd9
[alias]
	alias = !git config --list | grep 'alias\\.' | sed 's/alias\\.\\([^=]*\\)=\\(.*\\)/\\1\\\t => \\2/' | sort
	br = branch
	cl = clean -dfx
	cm = commit -m
	co = checkout
	d = diff
	dc = diff --cached --ignore-space-at-eol -b -w --ignore-blank-lines
	ds = diff --stat --ignore-space-at-eol -b -w --ignore-blank-lines
	dt = difftool
	f = fetch --prune
	latest = for-each-ref --sort=-committerdate --format='%(committerdate:short) %(refname:short)'
	lg = log --color --graph --pretty=format:'%C(magenta)%h%Creset%C(yellow)%d%Creset %s (%C(blue)%an%Creset %Cgreen%cr%Creset)'
	ll = log --stat --abbrev-commit
	ls = ls-files
	mv = branch -m
	p = pull --rebase --prune
	pu = push -u origin HEAD
	r = rebase @{u}
	ri = rebase -i @{u}
	s = status -sb
	sa = stash apply
	sd = stash drop
	sl = stash list
	sp = stash pop
	ss = stash save
	st = status
[merge]
	ff = only
	conflictstyle = diff3
[core]
	autocrlf = input
	editor = emacs -nw -q
	excludesfile = ~/.gitignore_global
[branch]
	autosetuprebase = always
[push]
	default = simple
	followTags = true
[rebase]
	autostash = true
[color]
	ui = auto
[rerere]
	enabled = true
